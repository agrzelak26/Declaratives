diff(red, green).
diff(green, red).
diff(red, blue).
diff(blue, red).
diff(green, blue).
diff(blue, green).

color(Podk, Lubl, Mazo, Podl, Warm) :-
  diff(Podk, Lubl),
  diff(Lubl, Mazo),
  diff(Lubl, Podl),
  diff(Mazo, Podl),
  diff(Mazo, Warm),
  diff(Podl, Warm).

?- color(Podk, Lubl, Mazo, Podl, Warm).
